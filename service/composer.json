{
    "name": "laravel/lumen",
    "description": "Lumen 8 - demo project",
    "keywords": [
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/mail": "^8.12",
        "illuminate/redis": "^8.1.0",
        "laravel/lumen-framework": "^8.0",
        "predis/predis": "^1.1.1",
        "ext-openssl": "*",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "aws/aws-sdk-php-resources": "*",
        "doctrine/dbal": "*",
        "goaop/goaop-laravel-bridge": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.13",
        "phpunit/phpunit": "^9.0",
        "zircote/swagger-php": "^3.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
